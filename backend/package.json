{
  "name": "tolls",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npx hardhat compile",
    "deploy-testnet": "npx hardhat run ./scripts/Toll_deploy.js --network MATIC_MUMBAI",
    "deploy": "npx hardhat run ./scripts/Toll_deploy.js --network MATIC_MAINNET",
    "deploy-Toll": "npx hardhat run ./scripts/Toll_deploy.js --network MATIC_MAINNET",
    "deploy-TollsMain": "npx hardhat run ./scripts/TollsMain_deploy.js --network MATIC_MAINNET",
    "node": "npx hardhat node",
    "deploy-local": "npx hardhat run ./scripts/Toll_deploy.js --network localhost"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "dotenv": "^16.0.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "hardhat": "^2.10.1"
  }
}
